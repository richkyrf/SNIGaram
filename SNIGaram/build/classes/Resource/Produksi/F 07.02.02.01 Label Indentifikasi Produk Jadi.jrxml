	<queryString>
		<![CDATA[SELECT DATE_FORMAT(`Tanggal`,'%d/%m/%Y') AS 'Tanggal', IF(`NamaBarang` LIKE '%G. KASAR%',REPLACE(`NamaBarang`,'G. KASAR','Garam Kasar Tiga Bola'),REPLACE(`NamaBarang`,'G. HALUS','Garam Halus Dua Angso')) AS 'Jenis Produk', CONCAT(REPLACE(FORMAT(SUM(`JumlahHasil`),0),',','.'),' Pak') AS 'Jumlah', 'C1234' AS 'Kode Produksi', '' AS 'Keterangan' FROM `tbpacking` AS A JOIN `tbmbarang` AS B ON A.`IdBarangHasil`=B.`IdBarang` GROUP BY `Tanggal`, `NamaBarang`]]>
