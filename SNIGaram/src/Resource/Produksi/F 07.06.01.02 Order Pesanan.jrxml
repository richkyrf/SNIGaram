	<parameter name="Tanggal" class="java.util.Date" isForPrompting="false">
		<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="Tanggal_Awal" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["2018-01-01"]]></defaultValueExpression>
	</parameter>
	<parameter name="Tanggal_Akhir" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["2018-12-31"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT * FROM (
SELECT B.`Tanggal`, SUM(`KarungPelita`) AS 'Jumlah Produk', 'Sak' AS 'Satuan', IF(`NamaBarang` LIKE '%GRM KSR%',REPLACE(`NamaBarang`,`NamaBarang`,'Garam Kasar 50Kg'),IF(`NamaBarang` LIKE '%GRM HLS%',REPLACE(`NamaBarang`,`NamaBarang`,'Garam Halus 50Kg'),'')) AS 'Jenis Produk', '1 Bulan' AS 'Jangka Waktu', '' AS 'Keterangan' FROM `tbpenerimaan` AS A JOIN `tbmpartai` AS B ON A.`IdPartai`=B.`IdPartai` JOIN `tbmbarang` AS C ON B.`IdBarang`=C.`IdBarang` WHERE `IdPeminta`=1 AND B.`Tanggal` BETWEEN $P{Tanggal_Awal} AND $P{Tanggal_Akhir} GROUP BY `Tanggal`, `Jenis Produk`
UNION ALL
SELECT `Tanggal`, SUM(`Netto`) AS 'Jumlah Produk', UCWORDS(`Satuan`), REPLACE(REPLACE(UCWORDS(`NamaBarangLain`),'Dlm','Dalam'),'Grm','Garam') AS 'Jenis Produk', '1 Bulan' AS 'Jangka Waktu', '' AS 'Keterangan' FROM `tbpenerimaanlain` AS A JOIN `tbmbaranglain` AS B ON A.`IdBarangLain`=B.`IdBarangLain` WHERE `Tanggal` BETWEEN $P{Tanggal_Awal} AND $P{Tanggal_Akhir} GROUP BY `Tanggal`, `Jenis Produk`
) AS `tborderpesanan` ORDER BY `Tanggal`, `Jenis Produk`]]>
	</queryString>
