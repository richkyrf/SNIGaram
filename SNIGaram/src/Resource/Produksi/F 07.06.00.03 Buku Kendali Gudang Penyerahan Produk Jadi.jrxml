	<queryString>
		<![CDATA[SELECT `Tanggal` AS 'Tgl Order Pesanan', SUM(`Jumlah`) AS 'Jumlah Pesanan', REPLACE(REPLACE(REPLACE(IF(`NamaBarang` LIKE '%1/2 KG%' OR `NamaBarang` LIKE '%8 ONS%',REPLACE(REPLACE(`NamaBarang`,'G. HALUS','Garam Halus Duo Angso'),'G. KASAR','Garam Kasar Duo Angso'),REPLACE(REPLACE(`NamaBarang`,'G. HALUS','Garam Halus Tiga Bola'),'G. KASAR','Garam Kasar Tiga Bola')),'GRAM','Gram'),'ONS','Ons'),'KG','Kg') AS 'Jenis Pesanan', 'Stok Pal 5' AS 'Keterangan' FROM `tbpenjualan` AS A JOIN `tbpenjualandetail` AS B ON A.`NoTransaksi`=B.`NoTransaksi` JOIN `tbmbarang` AS C ON B.`IdBarang`=C.`IdBarang` WHERE A.`NoTransaksi` LIKE '%SJ%' AND `NamaBarang` NOT LIKE '%@50 KG%' AND `StatusRetur`=0 AND `Tanggal` BETWEEN $P{Tanggal_Awal} AND $P{Tanggal_Akhir} GROUP BY `Tanggal`, `Jenis Pesanan` ORDER BY `Tanggal`, `Jenis Pesanan`]]>
	</queryString>
