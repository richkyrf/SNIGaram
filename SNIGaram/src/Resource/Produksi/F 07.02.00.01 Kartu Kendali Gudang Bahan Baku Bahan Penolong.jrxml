	<parameter name="Tanggal" class="java.util.Date" isForPrompting="false">
		<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="Tanggal_Awal" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["2018-01-01"]]></defaultValueExpression>
	</parameter>
	<parameter name="Tanggal_Akhir" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["2018-12-31"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT A.`Tanggal`, IF(`NamaBarang` LIKE '%GRM KSR%',REPLACE(`NamaBarang`,`NamaBarang`,'Garam Kasar 50 Kg'),IF(`NamaBarang` LIKE '%GRM HLS%',REPLACE(`NamaBarang`,`NamaBarang`,'Garam Halus 50 Kg'),'')) AS 'Nama Barang', 'Garam' AS 'Jenis Barang', SUM(`KarungPelita`) AS 'Jumlah Barang', 'Sak' AS 'Satuan', IF(`NamaBarang` LIKE '%GARINDO%','PT. Garindo',IF(`NamaBarang` LIKE '%PERSERO%','PT. Persero',IF(`NamaBarang` LIKE '%GRM HLS%','PT. Garindo',''))) AS 'Supplier' FROM `tbpenerimaan` AS A JOIN `tbmpartai` AS B ON A.`IdPartai`=B.`IdPartai` JOIN `tbmbarang` AS C ON B.`IdBarang`=C.`IdBarang` WHERE A.`Tanggal` BETWEEN $P{Tanggal_Awal} AND $P{Tanggal_Akhir} AND `IdPeminta`=1 GROUP BY A.`IdPartai`, `Nama Barang`
UNION ALL
SELECT `Tanggal`, IF(`NamaBarangLain` LIKE '%PLASTIK LUAR%',REPLACE(`NamaBarangLain`,`NamaBarangLain`,'Plastik Luar'),IF(`NamaBarangLain` LIKE '%PLASTIK DLM%',REPLACE(`NamaBarangLain`,`NamaBarangLain`,'Plastik Dalam'),IF(`NamaBarangLain` LIKE '%IODIUM%',REPLACE(`NamaBarangLain`,`NamaBarangLain`,'Iodium'),IF(`NamaBarangLain` LIKE '%LAKBAN%',REPLACE(`NamaBarangLain`,`NamaBarangLain`,'Lakban'),IF(`NamaBarangLain` LIKE '%ARTHESS%',REPLACE(`NamaBarangLain`,`NamaBarangLain`,'Arthess'),`NamaBarangLain`))))) AS 'Nama Barang', IF(`JenisBarangLain`='PLASTIK DALAM' OR `JenisBarangLain`='PLASTIK LUAR','Plastik',IF(`JenisBarangLain`='IODIUM','Iodium',IF(`NamaBarangLain` LIKE '%LAKBAN%',REPLACE(`NamaBarangLain`,`NamaBarangLain`,'Lakban'),IF(`NamaBarangLain` LIKE '%ARTHESS%',REPLACE(`NamaBarangLain`,`NamaBarangLain`,'Arthess'),`NamaBarangLain`)))) AS 'Jenis Barang', SUM(`Netto`) AS 'Jumlah Barang', IF(`Satuan`='KG','Kg',IF(`Satuan`='ROLL','Roll',IF(`Satuan`='GLN','Gln',`Satuan`))) AS 'Satuan', IFNULL(IF(`PemasokLain`='PT. GARINDO','PT. Garindo',IF(`PemasokLain`='PT. PERSERO','PT. Persero',IF(`PemasokLain`='WININDO JAYA','Winindo Jaya',`PemasokLain`))),'Trona JPM') AS 'Supplier' FROM `tbpenerimaanlain` AS A JOIN `tbmbaranglain` AS B ON A.`IdBarangLain`=B.`IdBarangLain` JOIN `tbsmjenisbaranglain` AS C ON B.`IdJenisBarangLain`=C.`IdJenisBarangLain` LEFT JOIN `tbmpemasoklain` AS D ON A.`IdPemasokLain`=D.`IdPemasokLain` WHERE `Tanggal` BETWEEN $P{Tanggal_Awal} AND $P{Tanggal_Akhir} GROUP BY `Tanggal`, `Nama Barang` ORDER BY `Tanggal`, `Nama Barang`]]>
	</queryString>
